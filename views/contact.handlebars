  <div class="container">

    <h1 class="brand">Olsen Day</h1>
    <div class="wrapper animated bounceInLeft">
      <div class="company-info">
        <h3>Olsen Day</h3>
        <h5>Olsen Day is located in the Ambrose Group building in northwest Houston.</h6>
        <ul>
          <li><i class="fa fa-road"></i> Olsen Day <br>
          16545 Village Drive <br>
          Building A, Suite 200 <br>
          Jersey Village, TX 77040</li>
          <li><i class="fa fa-phone"></i> (832) 795-2745</li>
          <li><i class="fa fa-fax"></i> (713) 688-1117</li>
          <li><i class="fa fa-envelope"></i> info@olsenday.com</li>
          <li><i class="fa fa-linkedin"></i><a href="https://www.linkedin.com/in/meday"> LinkedIn</a></li>
          <li><i class="fa fa-address-card-o"></i> <a href="public/assets/MarkDay.vcf">Download vCard</a></li>
        </ul>
      </div>
      <div class="contact">
        <h3>Email Us</h3>
        {{msg}}
        <form method="POST" action="/send">
          <p>
            <label>Name</label>
            <input type="text" name="name">
          </p>
          <p>
            <label>Company</label>
            <input type="text" name="company">
          </p>
          <p>
            <label>Email Address</label>
            <input type="email" name="email">
          </p>
          <p>
            <label>Phone Number</label>
            <input type="text" name="phone">
          </p>
          <p class="full">
            <label>Message</label>
            <textarea name="message" rows="5"></textarea>
          </p>
          <p class="full">
            <button type="submit">Submit</button>
          </p>
        </form>
      </div>
    </div>
  </div>
  <footer>
    <div class="container-fluid padding">
      <div class="row text-center">
        <div class="col-12">
